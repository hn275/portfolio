---
type Props = {
    title: string
}
const { title } = Astro.props as Props;
---
<div class="divider w-[90%] mx-auto" />
<section class="my-10 w-[100vw] pb-10">
    <div class="max-w-screen-xl mx-auto">
        <h2 class="text-2xl font-[600] text-primary text-center">{title}</h2>
        <slot />
    </div>
</section>
